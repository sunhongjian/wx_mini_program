/* pages/screen/screen.wxss */

.body {
  height: 100vh;
  overflow: hidden;
}

.selected {
  padding: 20rpx;
}

.border-top {
  border-top: 1px solid #cecfd6;
}

.selected text {
  font-size: 30rpx;
  font-weight: 600;
  color: #f7651b;
}

.selected view {
  color: #f8712d;
  margin-top: 5px;
}

.selected view text {
  font-size: 28rpx;
  color: #5c5c5c;
  margin: 0 3px;
}

/* 表格代码 */

.result {
  padding-bottom: 0px;
}

.b_head {
  border-bottom: 1px solid #dadada;
  border-top: 1px solid #dadada;
  padding: 10px 5px;
  font-size: 28rpx;
  color: #000;
  display: flex;
  align-items: center;
}

/* background-color: #e9f1fe; */

.table {
  border-left: 1px solid #dadada;
  position: relative;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  margin-bottom: 60rpx;
}

.table .table_c {
  display: flex;
  width: 100%;
}

.table .left {
  width: 100px;
}

.table .left .td {
  display: flex;
  align-items: center;
  height: 80rpx;
  border-bottom: 1px solid #cecfd6;
  border-right: 1px solid #cecfd6;
  padding: 10px 5px;
  font-size: 28rpx;
  color: #171f2c;
  background: #f6f6f6;
}

/* .table .left .td:nth-child(1){color:#fff;} */

/* .table .left .td:nth-child(1) .td_img{ opacity: 0;} */

.table .left .td .td_type {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp:  2;
  -webkit-box-orient: vertical;
  font-weight: 800;
}

.table .left .td .td_type.active {
  color: #f8712d;
}

.table .left .td .td_img {
  width: 15px;
}

.table .left .td .td_img image {
  width: 100%;
}

.table .left .td .td_type .text {
  font-size: 20rpx;
  color: #888887;
  width: 100%;
}

.table .right {
  white-space: nowrap;
  width: calc(100vw - 100px);
  margin-left: -1px;
}

.table .right .tr {
  font-size: 28rpx;
  color: #171f2c;
  display: inline-block;
}

.table .right .tr .td {
  width: 200rpx;
  display: flex;
  align-items: center;
  height: 80rpx;
  border-bottom: 1px solid #cecfd6;
  border-right: 1px solid #cecfd6;
  padding: 10px 5px;
}

.table .right .tdr {
  display: flex;
  width: 200rpx;
  align-items: center;
  height: 80rpx;
  border-bottom: 1px solid #cecfd6;
  border-right: 1px solid #cecfd6;
  padding: 10px 5px;
}

.table .right .tdr view {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  display: block;
}

.table .right .td .td_type {
  width: 100%;
  height: 100%;
  position: relative;
  overflow: hidden;
}

.table .right .td .td_type text {
  white-space: pre-line;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}

.table .right .td.active {
  color: #8a8a8a;
}

/* .table .right .tr:nth-child(1) .td {
  border-left: 1px solid #cecfd6;
} */

/* .table .right .tr .td:nth-child(1){background-color: #e9f1fe;font-size: 28rpx;color: #f8712d; position: absolute;top:0;} */

.result .tips {
  font-size: 28rpx;
  color: #c4c4c4;
  padding: 20px 0 50px;
  text-align: center;
}

.nav {
  background: #fff;
  position: fixed;
  bottom: 0;
  box-shadow: 0px 0px 18px 0px rgba(134, 134, 134, 0.75);
  width: 100%;
  display: flex;
  align-items: center;
  height: 130rpx;
  z-index: 10;
}

.nav button::after {
  border: none;
}

.nav button image {
  width: 20px;
  margin-bottom: -3px;
}

.nav image.shu {
  width: 2px;
}

/* 二期新增功能样式 */

.totle-nav {
  margin: 0;
  padding: 0;
  overflow: hidden;
  white-space: nowrap;
  position: relative;
  height: 60rpx;
  border-bottom: 1px solid #f0f0f0;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.totle-nav .scroll-wrap {
  white-space: nowrap;
  padding: 0 5px;
  display: inline-block;
  font-size: 32rpx;
  width: 100%;
  box-sizing: border-box;
  color: rgba(0, 0, 0, 0.8);
}

.totle-nav .scroll-wrap text {
  display: inline-block;
  padding: 5px;
  margin: 0 5px;
  box-sizing: border-box;
}

.totle-nav .scroll-wrap text.active {
  border-bottom: 3px solid rgba(255, 255, 255, 0.7);
}

.totle-nav .dec-image {
  position: absolute;
  right: 0;
  bottom: 0rpx;
  width: 80rpx;
  height: 60rpx;
  background: rgba(255, 255, 255, 0.8);
  display: flex;
  justify-content: center;
  align-items: center;
}

.totle-nav .dec-image image {
  width: 38rpx;
}

.nav .icon-style {
  width: 34rpx;
}

.nav text {
  font-size: 30rpx;
  color: #f7651b;
}

.nav .nav-item {
  width: 100%;
  height: 130rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.phone-icon {
  width: 40rpx;
}

.phoneItem {
  padding: 10rpx;
  border-bottom: 1px solid #eee;
}

.phoneItem:last-child {
  border: none;
}

.phoneItem text {
  color: #333;
  font-size: 32rpx;
}

.mohu {
  filter: blur(2px);
}

.zkqb {
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-left: -200rpx;
  color: #f7651b;
  font-size: 30rpx;
}

#loading {
  background-color: #fff;
  height: 100%;
  width: 100%;
  position: fixed;
  z-index: 99999;
  margin-top: 0px;
  top: 0px;
}

#loading-center {
  width: 100%;
  height: 100%;
  position: relative;
}

#loading-center-absolute {
  position: absolute;
  left: 50%;
  top: 50%;
  height: 150px;
  width: 150px;
  margin-top: -75px;
  margin-left: -75px;
  -ms-transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}

.object {
  width: 20px;
  height: 20px;
  background-color: #f7651b;
  position: absolute;
  left: 65px;
  top: 65px;
}

.object:nth-child(2n+0) {
  margin-right: 0px;
}

#object_one {
  -webkit-animation: object_one 2s infinite;
  animation: object_one 2s infinite;
  -webkit-animation-delay: 0.2s;
  animation-delay: 0.2s;
}

#object_two {
  -webkit-animation: object_two 2s infinite;
  animation: object_two 2s infinite;
  -webkit-animation-delay: 0.3s;
  animation-delay: 0.3s;
}

#object_three {
  -webkit-animation: object_three 2s infinite;
  animation: object_three 2s infinite;
  -webkit-animation-delay: 0.4s;
  animation-delay: 0.4s;
}

#object_four {
  -webkit-animation: object_four 2s infinite;
  animation: object_four 2s infinite;
  -webkit-animation-delay: 0.5s;
  animation-delay: 0.5s;
}

#object_five {
  -webkit-animation: object_five 2s infinite;
  animation: object_five 2s infinite;
  -webkit-animation-delay: 0.6s;
  animation-delay: 0.6s;
}

#object_six {
  -webkit-animation: object_six 2s infinite;
  animation: object_six 2s infinite;
  -webkit-animation-delay: 0.7s;
  animation-delay: 0.7s;
}

#object_seven {
  -webkit-animation: object_seven 2s infinite;
  animation: object_seven 2s infinite;
  -webkit-animation-delay: 0.8s;
  animation-delay: 0.8s;
}

#object_eight {
  -webkit-animation: object_eight 2s infinite;
  animation: object_eight 2s infinite;
  -webkit-animation-delay: 0.9s;
  animation-delay: 0.9s;
}

#object_big {
  position: absolute;
  width: 50px;
  height: 50px;
  left: 50px;
  top: 50px;
  -webkit-animation: object_big 2s infinite;
  animation: object_big 2s infinite;
  -webkit-animation-delay: 0.5s;
  animation-delay: 0.5s;
}

@-webkit-keyframes object_big {
  50% {
    -webkit-transform: scale(0.5);
  }
}

@keyframes object_big {
  50% {
    transform: scale(0.5);
    -webkit-transform: scale(0.5);
  }
}

@-webkit-keyframes object_one {
  50% {
    -webkit-transform: translate(-65px, -65px);
  }
}

@keyframes object_one {
  50% {
    transform: translate(-65px, -65px);
    -webkit-transform: translate(-65px, -65px);
  }
}

@-webkit-keyframes object_two {
  50% {
    -webkit-transform: translate(0, -65px);
  }
}

@keyframes object_two {
  50% {
    transform: translate(0, -65px);
    -webkit-transform: translate(0, -65px);
  }
}

@-webkit-keyframes object_three {
  50% {
    -webkit-transform: translate(65px, -65px);
  }
}

@keyframes object_three {
  50% {
    transform: translate(65px, -65px);
    -webkit-transform: translate(65px, -65px);
  }
}

@-webkit-keyframes object_four {
  50% {
    -webkit-transform: translate(65px, 0);
  }
}

@keyframes object_four {
  50% {
    transform: translate(65px, 0);
    -webkit-transform: translate(65px, 0);
  }
}

@-webkit-keyframes object_five {
  50% {
    -webkit-transform: translate(65px, 65px);
  }
}

@keyframes object_five {
  50% {
    transform: translate(65px, 65px);
    -webkit-transform: translate(65px, 65px);
  }
}

@-webkit-keyframes object_six {
  50% {
    -webkit-transform: translate(0, 65px);
  }
}

@keyframes object_six {
  50% {
    transform: translate(0, 65px);
    -webkit-transform: translate(0, 65px);
  }
}

@-webkit-keyframes object_seven {
  50% {
    -webkit-transform: translate(-65px, 65px);
  }
}

@keyframes object_seven {
  50% {
    transform: translate(-65px, 65px);
    -webkit-transform: translate(-65px, 65px);
  }
}

@-webkit-keyframes object_eight {
  50% {
    -webkit-transform: translate(-65px, 0);
  }
}
@keyframes object_eight {
  50% {
    transform: translate(-65px, 0);
    -webkit-transform: translate(-65px, 0);
  }
}
